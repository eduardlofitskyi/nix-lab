<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="WARN">
    <Appenders>

        <File name="ToFile" fileName="./log.log">
            <PatternLayout>
                <Pattern>%d %p %c{1.} [%t] %m%n</Pattern>
            </PatternLayout>
        </File>

    </Appenders>
    <Loggers>

        <Root level="trace">
            <AppenderRef ref="ToFile"/>
        </Root>

    </Loggers>
</Configuration>